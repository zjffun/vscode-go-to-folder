{
  "name": "gotofolder",
  "displayName": "Go to Folder",
  "description": "",
  "keywords": [],
  "version": "0.0.1",
  "publisher": "zjffun",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zjffun/vscode-go-to-folder.git"
  },
  "bugs": {
    "url": "https://github.com/zjffun/vscode-go-to-folder/issues"
  },
  "homepage": "https://github.com/zjffun/vscode-go-to-folder#readme",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "gotofolder.quickOpenFolder",
          "group": "4_search@99",
          "when": "explorerResourceIsFolder"
        }
      ],
      "commandPalette": [
        {
          "command": "gotofolder.quickOpenFolder",
          "when": "workspaceFolderCount >= 1"
        }
      ]
    },
    "commands": [
      {
        "command": "gotofolder.quickOpenFolder",
        "title": "Go to Folder..."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "lint": "eslint src --ext ts",
    "deploy": "vsce publish --yarn",
    "_vscode:pack": "vsce package",
    "_vscode:install": "ls gotofolder-*.vsix | xargs code --install-extension",
    "_vscode:uninstall": "code --uninstall-extension zjffun.gotofolder"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "vsce": "^1.99.0",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {},
  "icon": "images/icon.png"
}
